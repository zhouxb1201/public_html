page{
  background-color: #f8f8f8;
  font-size: 28rpx;
}
.group{
  background-color: #fff;
  margin-bottom: 20rpx;
}
.cell{
  display: flex;
  justify-content: space-between;
  padding: 20rpx 30rpx;
  border-bottom: 1px solid #ebedf0;
  align-items: center;
}
.cell_l{
  color: #323233;
}
.cell_r{
  display: flex;
  align-items: center;
}
.cell_r .icon{
  color: #909399;  
}
.cell_r .user_img{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  overflow: hidden;
}
.cell_r .user_img image{
  width: 100%;
  height: 100%;
}



.foot{
  padding: 20rpx 30rpx;
  margin-top: 60rpx;
}
.foot .foot_btn{
  background-color: #f44;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 20rpx;
}