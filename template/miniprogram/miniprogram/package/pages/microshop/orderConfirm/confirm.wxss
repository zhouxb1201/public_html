page {
  font-size: 28rpx;
  background-color: #f8f8f8;
}

.useraddress {
  padding: 30rpx;
  position: relative;
  background-color: #fff;
}

.cart_bottom_bg::before {
  content: '';
  left: 0;
  right: 0;
  bottom: 0;
  height: 2px;
  position: absolute;
  background: repeating-linear-gradient(-45deg, #ff6c6c, #ff6c6c 20%, transparent 0, transparent 25%, #3283fa 0, #3283fa 45%, transparent 0, transparent 50%);
  background-size: 80px;
}

.no_address .adress_text {
  height: 60rpx;
  line-height: 60rpx;
  display: inline-block;
  margin-left: 80rpx;
  font-size: 28rpx;
}

.no_address .icon_add {
  position: absolute;
  left: 30rpx;
  bottom: 30rpx;
}

.no_address .icon_arrow {
  position: absolute;
  right: 30rpx;
  bottom: 40rpx;
}

.has_address {
  position: relative;
  font-size: 24rpx;
}

.has_address .icon_location {
  position: absolute;
  left: 0;
  top: 10rpx;
}

.has_address .icon_arrow {
  position: absolute;
  right: 0;
  top: 10rpx;
}

.has_address .address_box {
  margin-left: 70rpx;
}

.has_address .address_box .consigner {
  margin-right: 70rpx;
}

.popup_title{
  padding: 20rpx 30rpx;  
  border-bottom: 1px solid rgba(204, 204, 204, 0.226);
  position: relative;
}
.popup_title .addressClose{
  position: absolute;
  left: 30rpx;
  color: #1989fa;
}
.popup_title .addressTitle{
  text-align: center;
}
.popup_title .addressClose .icon_left{
  font-size: 24rpx;
}
.popup_info{
  max-height: 780rpx;
  overflow-y: auto;
  font-size: 24rpx;  
}
.popup_info .item{
  padding: 20rpx 30rpx;
  display: flex;
  align-items: center;
  justify-content: left;
  border-bottom: 1px solid rgba(204, 204, 204, 0.226);
}
.popup_info .item .item_info{
  margin-left: 20rpx;
}
.popup_info .item .cell{
  margin: 10rpx 0;
}
.close{
  padding: 40rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}
.close .closeBtn{
  background-color: #f44;
  border: 1rpx solid #f44;
  color: #fff;
  border-radius: 50rpx;
  font-size: 28rpx;
}

.confrim-wrap {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  position: relative;
  min-height: 140rpx;
  background-color: #fff;
  padding: 20rpx;
}

.confrim-left {
  flex: 1;
  display: flex;
  display: -webkit-box;
  display: -ms-flexbox;
}

.confrim-left .img {
  width: 120rpx;
  height: 120rpx;
  position: relative;
  overflow: hidden;
}

.confrim-left .img img {
  width: 100%;
  height: auto;
}

.confrim-goods-info {
  flex: 1;
  padding-left: 20rpx;
  font-size: 28rpx;
}

.confrim-goods-info div:last-child {
  color: #b5b5b5;
  margin-top: 8rpx;
}

.confrim-right {
  color: #fd5b5b;
  font-size: 28rpx;
}

.btn-receive {
  left: 0;
  bottom: 0;
  width: 100%;
  z-index: 100;
  position: fixed;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}

.btn-receive-wrap {
  height: 100rpx;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  font-size: 28rpx;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  background-color: #fff;
}

.action-btn {
  font-size: 28rpx;
  height: 80rpx;
  line-height: 76rpx;
  margin: 10rpx 20rpx;
  border-radius: 16rpx;
  color: #fff;
  background-color: #f44;
  text-align: center;
  width: 220rpx;
  border: 0;
  appearance: none;
  -webkit-appearance: none;
}

.shop_box {
  padding-bottom: 130rpx;
}

.shop {
  margin: 20rpx;
  border-radius: 20rpx;
  overflow: hidden;
  box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.05);
}

.shop .shopName {
  padding: 20rpx 30rpx;
  background-color: #fff;
  position: relative;
}

.shop .shopName .icon_shop {
  position: absolute;
  left: 30rpx;
  bottom: 15rpx;
}

.shop .shopName .text_name {
  margin-left: 40rpx;
}

.shopGood {
  padding: 20rpx 30rpx;
  font-size: 24rpx;
  border-bottom: 1px solid rgba(204, 204, 204, 0.226);
  position: relative;
  background-color: #fff;
  display: flex;
}

.shopGood .goodImg {
  width: 180rpx;
  height: 180rpx;
  text-align: center;
  line-height: 180rpx;
  overflow: hidden;
}

.shopGood .good_box {
  margin-left: 20rpx;
  display: flex;
  flex: 1;
  flex-direction: column;
  position: relative;
}

.shopGood .good_box .good_l .goodname {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.shopGood .good_box .good_l .goodspec {
  margin-right: 10rpx;
  color: rgba(117, 116, 116, 0.932);
}

.shopGood .good_box .good_l .goodspec text {
  margin: 0 5rpx;
}

.shopGood .good_box .good_r {
  position: absolute;
  bottom: 0;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.shopGood .good_box .good_r .good_price {
  color: #f44;
  font-weight: 700;
  width: 46%;
}

.shopGood .good_box .good_r .goodnum {
  color: #333;
}

.shop .address {
  background-color: #fff;
  padding: 20rpx 30rpx;
  display: flex;
  border-bottom: 1px solid rgba(204, 204, 204, 0.226);
}

.address .addressText_l {
  width: 50%;
}

.address .addressText_r {
  width: 50%;
  display: flex;
  justify-content: flex-end;
  font-size: 24rpx;
  color: #909399;
}

.address .addressText_r .arrow {
  margin-left: 15rpx;
}

.has_address {
  position: relative;
  font-size: 24rpx;
}

.has_address .icon_location {
  position: absolute;
  left: 0;
  top: 10rpx;
}

.has_address .icon_arrow {
  position: absolute;
  right: 0;
  top: 10rpx;
}

.has_address .address_box {
  margin-left: 70rpx;
}

.has_address .address_box .consigner {
  margin-right: 70rpx;
}

.group_box {
  background-color: #fff;
  margin: 20rpx;
  border-radius: 20rpx;
  overflow: hidden;
  box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.05);
}

.group_box .cell {
  line-height: 48rpx;
  padding: 20rpx 30rpx;
}

.group_box .cell .item {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.group_box .cell .item .value {
  font-size: 24rpx;
  color: #f44;
}

.group_box .cell .title {
  display: flex;
  align-items: center;
}

.group_box .cell .title .point_text {
  color: #909399;
  font-size: 24rpx;
  padding-left: 20rpx;
}

.point_checkbox {
  display: inline-block;
}

.coupon {
  background-color: #fff;
  padding: 20rpx 30rpx;
  display: flex;
  border-bottom: 1px solid rgba(204, 204, 204, 0.226);
}

.coupon .coupon_l {
  width: 50%;
}

.coupon .coupon_r {
  width: 50%;
  display: flex;
  justify-content: flex-end;
  font-size: 24rpx;
  color: #909399;
}

.coupon .coupon_r .arrow {
  margin-left: 15rpx;
  vertical-align: middle;
}

.usersay {
  background-color: #fff;
  padding: 20rpx 30rpx;
  display: flex;
  border-bottom: 1px solid rgba(204, 204, 204, 0.226);
}

.usersay .usersay_l {
  width: 30%;
}

.usersay .usersay_r {
  width: 70%;
}

.goodsnum {
  background-color: #fff;
  padding: 20rpx 30rpx;
  display: flex;
  justify-content: flex-end;
}

.goodsnum .amount {
  margin-left: 20rpx;
}

.goodsnum .amount text {
  color: #ff6c6c;
  font-weight: 700;
}

.van-submit-bar {
  left: 0;
  bottom: 0;
  width: 100%;
  z-index: 10;
  position: fixed;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  background-color: #fff;
}

.van-submit-bar__bar {
  height: 100rpx;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  font-size: 28rpx;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
}

.van-submit-bar__text {
  -webkit-box-flex: 1;
  -ms-flex: 1;
  flex: 1;
  font-weight: 500;
  text-align: right;
  color: #323233;
  padding-right: 24rpx;
}

.van-submit-bar__text text {
  display: inline-block;
}

.van-submit-bar__price {
  color: #f44;
  font-size: 36rpx;
  font-weight: 800;
}
