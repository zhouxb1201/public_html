page{
  background-color: #f8f8f8;
  font-size: 28rpx;
}

.cell{
  margin: 20rpx;
}

.cell .status{
  background-color: #fff;
  padding: 40rpx;
  display: flex;
  flex-direction: column;  
  justify-content: center;
  text-align: center;
}
.cell .status .icon{
  margin-bottom: 30rpx;
}
.cell .status .foot_group{
  padding: 20rpx;
}
.cell .status .foot_group .reset_btn{
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #f44;
  color: #fff;
  border-radius: 50rpx;
  text-align: center;
}

.cell .status .url{
  background-color: #ccc;
  color: #fff;
  margin: 20rpx;
  padding: 10rpx;
}