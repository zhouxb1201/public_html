page{
  font-size: 28rpx;
  background-color: #f8f8f8;
}

.cell_group .cell_item{
  background-color: #fff;
  display: flex;
  align-items: center;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  color: #323233;
  border-bottom: 1px solid #ebedf0;
}
.cell_item .cell_title{
  max-width: 200rpx;
  text-align: left;
  flex: 1;  
}
.cell_item .cell_value{
  flex: 1;
}
.cell_item .cell_value label{
  margin-left: 30rpx;
}
.cell_item .cell_value label radio .wx-radio-input{
  height: 35rpx; 
  width: 35rpx;
}
.shipping_tip{
  padding: 20rpx 30rpx;
  background-color: #fff7cc;
  color: #f56723;
  font-size: 24rpx;
}

.useraddress{
  padding: 30rpx;
  position: relative;
  background-color: #fff;
}
.no_address .adress_text{
  height: 60rpx;
  line-height: 60rpx;
  display: inline-block;
  margin-left: 80rpx;
}
.no_address .icon_add{
  position: absolute;
  left: 30rpx;
  bottom: 30rpx;
}
.no_address .icon_arrow{
  position: absolute;
  right: 30rpx;
  bottom: 40rpx
}

.cart_bottom_bg::before{
  content: '';
  left: 0;
  right: 0;
  bottom: 0;
  height: 2px;
  position: absolute;
  background: repeating-linear-gradient(-45deg,#ff6c6c,#ff6c6c 20%,transparent 0,transparent 25%,#3283fa 0,#3283fa 45%,transparent 0,transparent 50%);
  background-size: 80px;
}

.shop_box{
  padding-bottom: 130rpx;
}
.shop{
  margin: 20rpx;
  border-radius: 20rpx;
  overflow: hidden;
  box-shadow: 0 0 10px 0 rgba(0,0,0,.05);

}
.shop .shopName{
  padding: 20rpx 30rpx;
  background-color: #fff;
  position: relative;
}
.shop .shopName .icon_shop{
  position: absolute;
  left: 30rpx;
  bottom: 15rpx;
}
.shop .shopName .text_name{
  margin-left: 40rpx;
}

.shopGood{
  padding: 20rpx 30rpx;
  font-size: 24rpx;  
  border-bottom: 1px solid rgba(204, 204, 204, 0.226);
  position: relative;
  background-color: #fff;
  display: flex;
}
.shopGood .goodImg{
  width: 180rpx;
  height: 180rpx;
  text-align: center;
  line-height: 180rpx;
  overflow: hidden;
}
.shopGood .good_box{
  margin-left: 20rpx;
  display: flex;
  flex: 1;
  flex-direction: column;
  position: relative;
}
.shopGood .good_box .good_l .goodname{
  overflow:hidden;
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
}
.shopGood .good_box .good_l .goodspec{
  margin-right: 10rpx;
  color: rgba(117, 116, 116, 0.932);
}
.shopGood .good_box .good_l .goodspec text{
  margin: 0 5rpx;
}
.shopGood .good_box .good_r{  
  position: absolute;
  bottom: 0;
  width:100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.shopGood .good_box .good_r .good_price{
  color: #f44;
  font-weight: 700;
}
.shopGood .good_box .good_r .goodnum{
  color: rgba(184, 180, 180, 0.89);
}

.shop .address{
  background-color: #fff;
  padding: 20rpx 30rpx;
  display: flex;
  border-bottom: 1px solid rgba(204, 204, 204, 0.226);
}
.address .addressText_l{
  width: 50%;
}
.address .addressText_r{
  width: 50%;
  display: flex;
  justify-content: flex-end;
  font-size: 24rpx;
  color: #909399;
}
.address .addressText_r .arrow{
  margin-left: 15rpx;
}

.has_address{
  position: relative;
  font-size: 24rpx;
}
.has_address .icon_location{
  position: absolute;
  left: 0;
  top: 10rpx;
}
.has_address .icon_arrow{
  position: absolute;
  right: 0;
  top: 10rpx;
}
.has_address .address_box{
  margin-left: 70rpx;
}
.has_address .address_box .consigner{
  margin-right: 70rpx;
}

.group_box{
  background-color: #fff;
  margin: 20rpx;
  border-radius: 20rpx;
  overflow: hidden;
  box-shadow: 0 0 10px 0 rgba(0,0,0,.05);
}
.group_box .cell{
  line-height: 48rpx;
  padding: 20rpx 30rpx;
}
.group_box .cell .item{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.group_box .cell .item .value{
  font-size: 24rpx;
  color: #f44;
}
.group_box .cell .title{
  display: flex;
  align-items: center;
}
.group_box .cell .title .point_text{
  color: #909399;
  font-size: 24rpx;
  padding-left: 20rpx;
}
.point_checkbox{
  display: inline-block;
}

.coupon{
  background-color: #fff;
  padding: 20rpx 30rpx;
  display: flex;
  border-bottom: 1px solid rgba(204, 204, 204, 0.226);
}
.coupon .coupon_l{
  width: 50%;
}
.coupon .coupon_r{
  width: 50%;
  display: flex;
  justify-content: flex-end;
  font-size: 24rpx;
  color: #909399;
}
.coupon .coupon_r .arrow{
  margin-left: 15rpx;
  vertical-align: middle;
}

.usersay{
  background-color: #fff;
  padding: 20rpx 30rpx;
  display: flex;
  border-bottom: 1px solid rgba(204, 204, 204, 0.226);
}
.usersay .usersay_l{
  width: 30%;
}
.usersay .usersay_r{
  width: 70%;
}

.goodsnum{
  background-color: #fff;
  padding: 20rpx 30rpx;
  display: flex;
  justify-content: flex-end;
}
.goodsnum .amount{
  margin-left: 20rpx;
}
.goodsnum .amount text{
  color: #ff6c6c;
  font-weight: 700;
}

.footer{
  display: flex;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 99;
  background-color: #fff;
  width: 100%;
}
.footer .totalPrice{
  width: 70%;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.footer .totalPrice text{
  font-size: 34rpx;
  font-weight: 700;
  color: #f44;
  margin-right: 20rpx;
}
.footer .subbtn{
  width: 30%;
  background-color: #f44;
  border: 1px solid #f44;
  color: #fff;
  border-radius: 0;
  font-size: 36rpx;
}
.footer .subbtn.subing {
  background-color: rgb(122, 121, 121);
  border: 1px solid rgb(122, 121, 121);
}
.footer button::after{
  border: none;
}

/* 预售 */
.presell_box{
  background-color: #fff;
  padding: 20rpx 30rpx;
  border-bottom: 1px solid rgba(204, 204, 204, 0.226);
}
.presell_box .presell_item{
  display: flex;
  align-items: center;
  margin-bottom: 5rpx;
  font-size: 24rpx;  
  color: #909399
}
.presell_box .presell_item .sort{
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  background-color: #909399;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  margin-right: 12rpx;
}
.presell_box .presell_item .sort.red_sort{
  background-color: #f44;
}
.presell_box .presell_item .red_text{
  color: #f44;
}

/* 优惠券弹出框 */
.couponPup{
  padding:20rpx 30rpx 30rpx;
  font-size: 14px; 
  max-height: 780rpx;
  overflow-y:auto;    
}
.couponList{
  padding: 10px 0;
  border-bottom: 1px solid #eee;
  
}
.couponItem{
  display: inline-block;
}

.couponPup .couponTitle{
  color: #ff454e;
  font-weight:600; 
}
.couponPup .couponName{
  margin-right: 30rpx;
}
.couponPup .useDate{
  font-size: 12px;
  color: #999;
}
.couponPup .getBtn{  
  background-color: #ff454e;
  color: #fff;
  border-radius: 0;
  width: 120rpx;
  height: 60rpx;
  font-size: 24rpx;
  float: right;
  margin-top: 10rpx;
}
.couponPup .using{
  background-color: #e8e8e8;
  border: 1px solid #e5e5e5;
  color: #999;
}
.closebtn{
  background-color: #ff454e;
  color: #fff;
  font-size: 16px;
  border-radius: 0;
}
.no_more{
  padding: 30rpx;
  text-align: center;
  color:#86858596;
}
/* 没有数据 */
.no_cart{
  margin-top: 25%;
  padding: 30rpx;
  text-align: center;
}
.no_cart image{
  max-width: 65%;
  display: block;
  margin: 0 auto;
}
.no_cart .text_noData{
  margin: 30rpx 0;
  display: block;
}
.no_cart button{
  border-radius: none;
  width: 200rpx;
  height: 60rpx;
  font-size: 24rpx;
  background-color: #f44;
  color: #fff;
}
.no_cart button::after{
  border: 1px solid #f44;
}

.couponShopname{
  text-align: center;
  padding: 20rpx 30rpx;
  border-bottom: 1px solid #ebedf0;
}



.discount_group{
  padding: 20rpx 30rpx;
  background-color: #fff;
  border-bottom: 1px solid rgba(204, 204, 204, 0.226);
}
.discount_group .mansong{
  color: #909399;
  font-size: 24rpx;
}
.discount_group .mansong .item{
  display: flex;
  align-items: center;  
  margin: 8rpx 0;
}
.discount_group .mansong .item .tag{
  margin-right: 20rpx;  
}
.discount_group .mansong .item .tag_text{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  flex: 1;
}

.cover_box{
  z-index: 999;
}
.popup_title{
  padding: 20rpx 30rpx;  
  border-bottom: 1px solid rgba(204, 204, 204, 0.226);
  position: relative;
}
.popup_title .addressClose{
  position: absolute;
  left: 30rpx;
  color: #1989fa;
}
.popup_title .addressTitle{
  text-align: center;
}
.popup_title .addressClose .icon_left{
  font-size: 24rpx;
}
.popup_info{
  max-height: 1000rpx;
  overflow-y: auto;
  font-size: 24rpx;  
}
.popup_info .item{
  padding: 20rpx 30rpx;
  display: flex;
  align-items: center;
  justify-content: left;
  border-bottom: 1px solid rgba(204, 204, 204, 0.226);
}
.popup_info .item .item_info{
  margin-left: 20rpx;
}
.popup_info .item .cell{
  margin: 10rpx 0;
}
.close{
  padding: 40rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}
.close .closeBtn{
  background-color: #f44;
  border: 1rpx solid #f44;
  color: #fff;
  border-radius: 50rpx;
  font-size: 28rpx;
}