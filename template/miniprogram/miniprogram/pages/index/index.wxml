<view class="popup-customer" wx:if="{{customerImgUrl}}">
  <button open-type="contact" class="btn-customer">
    <image mode="widthFix" src="{{publicUrl}}{{customerImgUrl}}"></image>
  </button>
</view>

<!-- 滚动条返回到顶部 -->
<backtop-component backTopValue='{{backTopValue}}'></backtop-component>




